pipeline {
    agent any
	
	  environment {
		  POSTGRES_HOST = 'localhost'
		  POSTGRES_USER = 'myuser'
		  POSTGRES_DB = 'csv'
		  POSTGRES_PASSWORD = 'myPasswd'
		  
	  }

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}

