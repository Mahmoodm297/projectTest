pipeline {
    agent any
	
	environment {
		BRANCH = 'main'
		POSTGRES_DB = 'csvDB'
		Port = '5432'
		PGDATA = '/local/DB'
		POSTGRES_HOST = 'localhost'

	}

	stages {
	
    		stage ('retrieve git repo locally') {
			steps {
			      git branch: ${env.BRANCH} , credentialsId: 'GitHub', url: 'https://github.com/Mahmoodm297/projectTest.git'
           		      sh "git checkout ${env.BRANCH}"
			}
          	}
	}
}
